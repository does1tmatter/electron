    "electron": "^26.3.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "less": "^4.2.0",
    "pinia": "^2.1.6",
    "postcss": "^8.4.31",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",
